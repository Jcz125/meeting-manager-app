<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="97.0" prefWidth="1500.0" stylesheets="@../../../../style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="eu.telecomnancy.profrdv.client.controllers.MenuController">
   <HBox prefHeight="100.0" prefWidth="1460.0">
      <children>
         <ImageView fitHeight="60.0" fitWidth="231.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@logo.png" />
            </image>
            <HBox.margin>
               <Insets bottom="20.0" left="100.0" right="100.0" top="20.0" />
            </HBox.margin>
         </ImageView>
         <Button fx:id="accueilButton" mnemonicParsing="false" onAction="#handleAccueilButton" prefHeight="30.0" prefWidth="205.0" text="Prendre un rendez-vous">
            <HBox.margin>
               <Insets left="50.0" right="100.0" top="30.0" />
            </HBox.margin>
         </Button>
         <Button fx:id="personnalButton" mnemonicParsing="false" onAction="#handlePersonalButton" text="Espace Personnel">
            <HBox.margin>
               <Insets right="80.0" top="30.0" />
            </HBox.margin>
         </Button>
      <Button fx:id="idButton" mnemonicParsing="false" onAction="#handleIdButton" text="Identification">
            <HBox.margin>
               <Insets right="80.0" top="30.0" />
            </HBox.margin>
         </Button>
         <Button fx:id="createButton" mnemonicParsing="false" onAction="#handleCreateButton" text="Creation de compte">
            <HBox.margin>
               <Insets right="80.0" top="30.0" />
            </HBox.margin>
         </Button>
      </children>
   </HBox>
</AnchorPane>
